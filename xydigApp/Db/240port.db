
record(bo, "$(P)out${N}") {
  field( DTYP, "XYCOM 240")
  field(SCAN, "Passive")
  field(OUT , "#C${C} S${N} @" )
  #field(PINI, "YES")
  field(VAL , 0 )
  field(ZNAM, "Zero")
  field(ONAM, "One")
}

record(bo, "$(P)out${N}:dir") {
  field( DTYP, "XYCOM 240 Port Direction")
  field(SCAN, "Passive")
  field(OUT , "#C${C} S${N} @" )
  field(PINI, "YES")
  field(VAL , 1 )
  field(ZNAM, "Input")
  field(ONAM, "Output")
}

record(bi, "$(P)in${N}") {
  field( DTYP, "XYCOM 240")
  field(SCAN, "1 seconds")
  field(OUT , "#C${C} S${N} @" )
  field(ZNAM, "Zero")
  field(ONAM, "One")
}
